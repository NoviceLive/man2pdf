T{
.BR lround (),
.BR lroundf (),
.BR lroundl (),
.br
.BR llround (),
.BR llroundf (),
.BR llroundl ()
T}	Thread safety	MT-Safe
.TE
.SH CONFORMING TO
C99, POSIX.1-2001, POSIX.1-2008.
.SH SEE ALSO
.BR ceil (3),
.BR floor (3),
.BR lrint (3),
